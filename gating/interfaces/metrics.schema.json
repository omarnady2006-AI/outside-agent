{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "schema_ok",
    "missing_rate_by_field",
    "duplicates_rate",
    "range_violations_count_by_field",
    "enum_violations_count_by_field",
    "postcheck_ok",
    "patterns_remaining_count"
  ],
  "properties": {
    "schema_ok": {
      "type": "boolean"
    },
    "missing_rate_by_field": {
      "type": "object",
      "additionalProperties": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      }
    },
    "duplicates_rate": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "range_violations_count_by_field": {
      "type": "object",
      "additionalProperties": {
        "type": "integer",
        "minimum": 0
      }
    },
    "enum_violations_count_by_field": {
      "type": "object",
      "additionalProperties": {
        "type": "integer",
        "minimum": 0
      }
    },
    "postcheck_ok": {
      "type": "boolean"
    },
    "patterns_remaining_count": {
      "type": "integer",
      "minimum": 0
    }
  }
}